{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2020",
    "lib": ["DOM", "ESNext", "DOM.Iterable"],
    //    "strict": true,
    "esModuleInterop": true,
    "incremental": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/*.vue", "src/*.ts"]
}
