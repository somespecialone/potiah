{
  "name": "potiah",
  "version": "1.0.0-alpha.1",
  "description": "Vue wrapper for precious Locomotive Scroll",
  "keywords": [
    "typescript",
    "vue",
    "vue3",
    "component",
    "scroll",
    "locomotive-scroll",
    "lenis"
  ],
  "homepage": "https://potiah.somespecial.one",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/somespecialone/potiah.git",
    "directory": "lib"
  },
  "bugs": "https://github.com/somespecialone/potiah/issues",
  "license": "MIT",
  "author": {
    "name": "Dmytro Tkachenko",
    "url": "https://somespecial.one"
  },
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/potiah.js",
      "require": "./dist/potiah.umd.cjs",
      "types": "./dist/index.d.ts",
      "style": "./dist/lenis.css"
    },
    "./dist/*": "./dist/*"
  },
  "source": "src/index.ts",
  "unpkg": "dist/potiah.umd.cjs",
  "main": "dist/potiah.js",
  "umd:main": "dist/potiah.umd.cjs",
  "module": "dist/potiah.js",
  "types": "dist/index.d.ts",
  "style": "dist/lenis.css",
  "scripts": {
    "prepack": "node ../scripts/prepack.mjs",
    "postpack": "node ../scripts/postpack.mjs",
    "build": "vite build && vue-tsc"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "locomotive-scroll": "5.0.0-beta.11"
  }
}
